        "projects": "dependencies"
      }
    ],
    "prepare": [
      {
        "target": "prepare",
        "projects": "dependencies"
      }
    ],
    "package": [
      {
        "target": "package",
        "projects": "dependencies"
      }
    ]
  },
  "affected": {
    "defaultBase": "main"
  },
  "workspaceLayout": {
    "libsDir": "",
    "appsDir": ""
  },
  "implicitDependencies": {
    "package.json": "*",
    ".eslintrc.json": "*"
  }
