{
  "extends": "@nrwl/workspace/presets/npm.json",
  "npmScope": "p2p",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "lint",
          "package",
          "prepare"
        ],
        "accessToken": "YjQ2YmZkYTctMzA3MC00YWEyLWEyNjYtZTQ1MjUzZGE2YTEwfHJlYWQtd3JpdGU=",
        "useDaemonProcess": true
      }
